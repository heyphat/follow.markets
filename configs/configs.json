{
  "env": "development",  
  "server": {
    "host": "",
    "port": 6868,
    "limit": 5,
    "timeout": {
      "read": 10,
      "write": 10,
      "idle": 10
    }
  },
  "datadog": {
    "host": "",
    "port": "8126",
